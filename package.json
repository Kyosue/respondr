{
  "name": "respondr",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "node --max-old-space-size=8192 ./node_modules/expo/bin/cli start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "node --max-old-space-size=8192 ./node_modules/expo/bin/cli start --android",
    "ios": "node --max-old-space-size=8192 ./node_modules/expo/bin/cli start --ios",
    "web": "node --max-old-space-size=8192 ./node_modules/expo/bin/cli start --web",
    "lint": "expo lint",
    "build:web": "npx expo export --platform web && node scripts/copy-ionicons-font.js",
    "deploy:firebase": "firebase deploy",
    "deploy:firebase:rules": "firebase deploy --only firestore:rules,storage:rules",
    "deploy:firebase:functions": "cd functions && npm run build && cd .. && firebase deploy --only functions",
    "deploy:firebase:hosting": "npm run build:web && firebase deploy --only hosting",
    "deploy:web": "npm run build:web && firebase deploy --only hosting",
    "deploy:all": "npm run build:web && firebase deploy",
    "build:android": "eas build --profile production --platform android",
    "build:android:preview": "eas build --profile preview --platform android",
    "build:android:dev": "eas build --profile development --platform android",
    "build:ios": "eas build --profile production --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "docx": "^9.5.1",
    "dotenv": "^17.2.2",
    "expo": "54.0.21",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.16",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-maps": "~0.12.8",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "file-saver": "^2.0.5",
    "firebase": "^12.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-offline-queue": "^0.0.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-svg-pan-zoom": "^0.1.2",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@firebase/app-types": "^0.9.3",
    "@firebase/auth-types": "^0.13.0",
    "@firebase/firestore-types": "^3.0.3",
    "@types/file-saver": "^2.0.7",
    "@types/firebase": "^2.4.32",
    "@types/react": "~19.1.0",
    "baseline-browser-mapping": "^2.9.11",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "typescript": "~5.9.0"
  },
  "private": true
}
